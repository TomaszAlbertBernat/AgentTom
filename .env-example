## AgentTom minimal environment template (local-first)
## Copy to ".env" and fill in values. Do NOT commit your ".env" file.

# Basic app configuration
NODE_ENV=development
LOG_LEVEL=INFO
PORT=3000
APP_URL=http://localhost:3000

# Auth mode (local or multiuser)
AUTH_MODE=local

# AI Providers
# Required (primary): Google Gemini API key
GOOGLE_API_KEY=
# Optional (fallback): OpenAI API key
OPENAI_API_KEY=

# Optional services (enable by setting the required keys)
# Web scraping (Firecrawl)
FIRECRAWL_API_KEY=
# Email (Resend)
RESEND_API_KEY=
# TTS (ElevenLabs)
ELEVENLABS_API_KEY=
# Linear
LINEAR_API_KEY=
# Spotify
SPOTIFY_CLIENT_ID=
SPOTIFY_CLIENT_SECRET=

# Advanced (optional overrides)
DEFAULT_LLM_PROVIDER=google
DEFAULT_TEXT_MODEL=gemini-2.5-flash
