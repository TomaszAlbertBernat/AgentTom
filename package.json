{
  "name": "agenttom",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "generate": "drizzle-kit generate",
    "migrate": "bun run src/database/migrate.ts",
    "seed": "bun run src/database/seed.ts",
    "setup:linear": "bun run scripts/setup-linear.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "build": "tsc --noEmit",
    "todo:split": "bun run scripts/splitMaster.ts",
    "todo:merge": "bun run scripts/mergeAgentUpdates.ts",
    "todo:import": "bun run scripts/importTodos.ts",
    "todo:hooks": "git config core.hooksPath .githooks"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.8",
    "@ai-sdk/openai": "^2.0.19",
    "@hono/node-server": "^1.14.1",
    "@hono/swagger-ui": "^0.5.1",
    "@hono/zod-openapi": "^0.19.6",
    "@hono/zod-validator": "^0.4.1",
    "@libsql/client": "^0.5.4",
    "@types/jsonwebtoken": "^9.0.9",
    "ai": "^5.0.21",
    "bcryptjs": "^3.0.2",
    "drizzle-orm": "^0.36.3",
    "glob": "^10.3.10",
    "hono": "^4.9.2",
    "jsonwebtoken": "^9.0.2",
    "marked": "^15.0.12",
    "node-html-parser": "^7.0.1",
    "uuid": "^11.0.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^22.9.0",
    "@types/uuid": "^9.0.8",
    "bun-types": "^1.1.34",
    "drizzle-kit": "^0.28.1",
    "typescript": "^5.3.3"
  }
}
